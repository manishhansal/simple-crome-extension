{
    "manifest_version": 2,
    "name": "Cat to Dog",
    "version": "1.0",
    "description": "Replaces all instances of the word cat with the word dog when a button is clicked.",
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["content_script.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  